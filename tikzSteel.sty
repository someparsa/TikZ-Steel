\usepackage{tikz}
\usetikzlibrary{calc}

%
% ===== ===== Channels
%

\newcommand{\csChannel}[7]{
	\draw [line width=#6 mm, red] (#7,0) -- (#4-#7,0) ;
	\draw [line width=#6 mm, red] (0,#7) -- (0,#1-#7) ;
	\draw [line width=#6 mm, red] (#7,#1) -- (#2-#7,#1) ;

	\draw [line width=#6 mm, blue] (0,#7) arc (180:270:#7);
	\draw [line width=#6 mm, blue] (0,#1-#7) arc (180:90:#7);

	\ifnum #3 > 0
    		\draw [line width=#6 mm, red] (#2,#1-#7) -- (#2,#1-#3) ;
		\draw [line width=#6 mm, blue] (#2-#7,#1) arc (90:0:#7);
	\fi

	\ifnum #5 > 0
    		\draw [line width=#6 mm, red] (#4,#7) -- (#4,#5) ;
		\draw [line width=#6 mm, blue] (#4-#7,0) arc (270:360:#7);
	\fi
}

%
% ===== ===== Box
%

\newcommand{\csBox}[4]{
	\draw [line width=#3 mm, red] (#4,0) -- (#2-#4,0) ;
	\draw [line width=#3 mm, red] (#4,#1) -- (#2-#4,#1) ;	
	\draw [line width=#3 mm, red] (0,#4) -- (0,#1-#4) ;
	\draw [line width=#3 mm, red] (#2,#4) -- (#2,#1-#4) ;
	
	\draw [line width=#3 mm, blue] (0,#4) arc (180:270:#4);
	\draw [line width=#3 mm, blue] (0,#1-#4) arc (180:90:#4);

	\draw [line width=#3 mm, blue] (#2-#4,0) arc (270:360:#4);
	\draw [line width=#3 mm, blue] (#2-#4,#1) arc (90:0:#4);
}

%
% ===== ===== Zee
%

\newcommand{\csZee}[7]{
	\draw [line width=#6 mm, red] (0,#7) -- (0,#1-#7) ;	
	\draw [line width=#6 mm, red] (#7,#1) -- (#2-#7,#1) ;
	\draw [line width=#6 mm, red] (-#7,0) -- (#7-#4,0) ;

	\draw [line width=#6 mm, blue] (0,#1-#7) arc (180:90:#7);
	\draw [line width=#6 mm, blue] (0,#7) arc (0:-90:#7);

	\ifnum #3 > 0
		\draw [line width=#6 mm, red] (#2,#1-#7) -- (#2,#1-#3) ;
		\draw [line width=#6 mm, blue] (#2-#7,#1) arc (90:0:#7);
	\fi

	\ifnum #5 > 0
		\draw [line width=#6 mm, red] (-#4,#7) -- (-#4,#5) ;
		\draw [line width=#6 mm, blue] (#7-#4,0) arc (270:180:#7);
	\fi
}

%
% ===== ===== L
%

\newcommand{\csL}[4]{
	\draw [line width=#3 mm, red] (0,#4) -- (0,#1) ;	
	\draw [line width=#3 mm, red] (#4,0) -- (#2,0) ;

	\draw [line width=#3 mm, blue] (0,#4) arc (180:270:#4);

}

%
% ===== ===== Tube
%
\newcommand{\csTube}[2]{
	\draw [line width=#2 mm, blue] (0,0) arc (0:360:#1);
}

%
% ===== ===== Welded I
%
\newcommand{\csWI}[6]{
	\draw [line width=#2 mm, red] (0,-0.5*#1) -- (0,0.5*#1);
	\draw [line width=#4 mm, blue] (-0.5*#3,-0.5*#1) -- (0.5*#3,-0.5*#1);
	\draw [line width=#6 mm, blue] (-0.5*#5,0.5*#1) -- (0.5*#5,0.5*#1);
}

%
% ===== ===== Universal beams and columns
%
\newcommand{\csUBC}[5]{
	\draw [line width=1 mm, red] (0,0) -- (#2,0);
	\draw [line width=1 mm, red] (0,#1) -- (#2,#1);

	\draw [line width=1 mm, red] (0,#3) -- (0.5*#2-0.5*#4-#5,#3);
	\draw [line width=1 mm, red] (0.5*#2+0.5*#4+#5,#3) -- (#2,#3);

	\draw [line width=1 mm, red] (0,#1-#3) -- (0.5*#2-0.5*#4-#5,#1-#3);
	\draw [line width=1 mm, red] (0.5*#2+0.5*#4+#5,#1-#3) -- (#2,#1-#3);
	
%	\draw [line width=0.5 mm, black, dashed] (0.5*#2,-1) -- (0.5*#2,#1+1);

	\draw [line width=1 mm, red] (0.5*#2-0.5*#4,#3+#5) -- (0.5*#2-0.5*#4,{#1 - (#3+#5)});
	\draw [line width=1 mm, red] (0.5*#2+0.5*#4,#3+#5) -- (0.5*#2+0.5*#4,{#1 - (#3+#5)});
	
	\draw [line width=1 mm, blue] (0.5*#2-0.5*#4-#5,#3) arc (270:360:#5);
	\draw [line width=1 mm, blue] (0.5*#2+0.5*#4+#5,#3) arc (270:180:#5);

	\draw [line width=1 mm, blue] (0.5*#2-0.5*#4-#5,#1-#3) arc (90:0:#5);
	\draw [line width=1 mm, blue] (0.5*#2+0.5*#4+#5,#1-#3) arc (90:180:#5);
	
	\draw [line width=1 mm, red] (0,0) -- (0,#3);
	\draw [line width=1 mm, red] (#2,0) -- (#2,#3);

	\draw [line width=1 mm, red] (0,#1-#3) -- (0,#1);
	\draw [line width=1 mm, red] (#2,#1-#3) -- (#2,#1);
}

%
% ===== ===== Tees cut from universal beams and columns
%
\newcommand{\csTU}[5]{
	\draw [line width=1 mm, red] (0,#1) -- (#2,#1);

	\draw [line width=1 mm, red] (0.5*#2-0.5*#4,0) -- (0.5*#2+0.5*#4,0);

	\draw [line width=1 mm, red] (0,#1-#3) -- (0.5*#2-0.5*#4-#5,#1-#3);
	\draw [line width=1 mm, red] (0.5*#2+0.5*#4+#5,#1-#3) -- (#2,#1-#3);

	\draw [line width=1 mm, red] (0.5*#2-0.5*#4,0) -- (0.5*#2-0.5*#4,{#1 - (#3+#5)});
	\draw [line width=1 mm, red] (0.5*#2+0.5*#4,0) -- (0.5*#2+0.5*#4,{#1 - (#3+#5)});

	\draw [line width=1 mm, blue] (0.5*#2-0.5*#4-#5,#1-#3) arc (90:0:#5);
	\draw [line width=1 mm, blue] (0.5*#2+0.5*#4+#5,#1-#3) arc (90:180:#5);

	\draw [line width=1 mm, red] (0,#1-#3) -- (0,#1);
	\draw [line width=1 mm, red] (#2,#1-#3) -- (#2,#1);
}

%
% ===== ===== HRS Channels
%
\newcommand{\csHCH}[5]{
	\draw [line width=1 mm, red] (0,0) -- (0,#1);
	
	\draw [line width=1 mm, red] (0,0) -- (#2,0);
	\draw [line width=1 mm, red] (0,#1) -- (#2,#1);

	\draw [line width=1 mm, red] (#4+#5,#3) -- (#2,#3);

	\draw [line width=1 mm, red] (#4+#5,#1-#3) -- (#2,#1-#3);
	
	\draw [line width=1 mm, red] (#4,#3+#5) -- (#4,{#1 - (#3+#5)});
	
	\draw [line width=1 mm, blue] (#4+#5,#3) arc (270:180:#5);

	\draw [line width=1 mm, blue] (#4+#5,#1-#3) arc (90:180:#5);
	
	\draw [line width=1 mm, red] (#2,0) -- (#2,#3);

	\draw [line width=1 mm, red] (#2,#1-#3) -- (#2,#1);
}

%
% ===== ===== HRS Angles
%
\newcommand{\csEA}[5]{
	\draw [line width=1 mm, red] (0,0) -- (0,#1);
	
	\draw [line width=1 mm, red] (0,0) -- (#2,0);
	\draw [line width=1 mm, red] (0,#1) -- (#4,#1);

	\draw [line width=1 mm, red] (#4+#5,#3) -- (#2,#3);

	\draw [line width=1 mm, red] (#4,#3+#5) -- (#4,#1);
	
	\draw [line width=1 mm, blue] (#4+#5,#3) arc (270:180:#5);

	
	\draw [line width=1 mm, red] (#2,0) -- (#2,#3);

}